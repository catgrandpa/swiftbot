{
  "name": "swiftbot",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "whenever you need to be told that you're really doing a whole lotta damage, phil swift (is) here for you!",
  "icons": {
    "4": "icons/image4.png",
    "3": "icons/image3.png",
  },
  "default_locale": "en",
  "permissions": [
    "activeTab",
    "tabs", "<all_urls>"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
     "default_icon": {
        "1": "icons/image1.png",
        "2": "icons/image2.png"
      },
    "default_title": "FLEX TAPE"
  },
  "web_accessible_resources": [
    "media/*"
  ]
  }
